{
  "_info": [
    {
      "func_args": {
        "task": "<function task_check(..)>"
      },
      "test_file": "test_doit_tasks.py",
      "test_name": "test_task_test"
    }
  ],
  "_json": {
    "actions": [
      [
        "<function if_found_unlink(..)>",
        [
          "--/.pft_flake8.log"
        ]
      ],
      "Cmd: poetry run flake8 game/tasks --config=.flake8 --output-file=.pft_flake8.log --color=never --exit-zero",
      [
        "<function if_found_unlink(..)>",
        [
          "--/.pft_pylint.json"
        ]
      ],
      "Cmd: poetry run pylint game/tasks --rcfile=.pylintrc --output-format=json --output=.pft_pylint.json --exit-zero",
      [
        "<function _merge_linting_logs(..)>",
        [
          "--/.pft_flake8.log",
          "--/.pft_pylint.json"
        ]
      ]
    ],
    "title": "<function user_task.<locals>.<lambda>(..)>",
    "verbosity": 2
  }
}
