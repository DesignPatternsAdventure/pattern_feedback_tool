{
  "_info": [
    {
      "func_args": {
        "task": "<function task__check(..)>"
      },
      "test_file": "test_doit_tasks.py",
      "test_name": "test_task_test"
    }
  ],
  "_json": {
    "actions": [
      [
        "<function if_found_unlink(..)>",
        [
          "--/.pft_flake8.log"
        ]
      ],
      "Cmd: .nox/coverage-3-10/bin/python -m flake8 tests game ./dodo.py --output-file=.pft_flake8.log --config=.flake8 --color=never --exit-zero",
      [
        "<function if_found_unlink(..)>",
        [
          "--/.pft_pylint.json"
        ]
      ],
      "Cmd: .nox/coverage-3-10/bin/python -m pylint tests game ./dodo.py --output=.pft_pylint.json --rcfile=.pylintrc --output-format=json --exit-zero",
      [
        "<function _merge_linting_logs(..)>",
        [
          "--/.pft_flake8.log",
          "--/.pft_pylint.json"
        ]
      ]
    ],
    "title": "<function _show_cmd(..)>",
    "verbosity": 2
  }
}
